.wd-upload{position:relative;display:flex;flex-wrap:wrap}.wd-upload__evoke{position:relative;display:inline-flex;flex-direction:column;justify-content:center;align-items:center;width:80px;height:80px;font-size:32px;background-color:rgba(0,0,0,.04);color:rgba(0,0,0,.25)}.wd-upload__evoke.is-disabled{color:rgba(0,0,0,.09)}.wd-upload__evoke.is-slot-default{width:auto;height:auto;background-color:transparent}.wd-upload__evoke-num{font-size:14px;line-height:14px;margin-top:8px}.wd-upload__evoke-icon{width:32px;height:32px}.wd-upload__input{position:absolute;top:0;left:0;width:100%;height:100%;overflow:hidden;opacity:0}.wd-upload__preview{position:relative;width:80px;height:80px;margin:0 12px 12px 0}.wd-upload__preview-list{display:flex}.wd-upload__picture{display:block;width:100%;height:100%}.wd-upload__close{position:absolute;right:-8px;top:-8px;font-size:16px;z-index:10;color:rgba(0,0,0,.65);width:16px;height:16px}.wd-upload__close:after{position:absolute;content:"";width:100%;height:100%;border-radius:50%;background-color:#fff;left:0;z-index:-1}.wd-upload__mask{position:absolute;top:0;left:0;background-color:rgba(0,0,0,.6)}.wd-upload__progress-txt{font-size:14px;line-height:14px;margin-top:9px;color:#fff}.wd-upload__icon{font-size:24px;color:#fff}.wd-upload__status-content{display:flex;flex-direction:column;justify-content:center;align-items:center;width:100%;height:100%}