.wd-collapse-item__header{position:relative;display:flex;justify-content:space-between;align-items:center;padding:13px 15px;overflow:hidden;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}.wd-collapse-item__title{color:rgba(0,0,0,.85);font-weight:500;font-size:16px}.wd-collapse-item__arrow{display:block;font-size:18px;color:#d8d8d8;transition:transform .3s}.wd-collapse-item__arrow.is-retract{transform:rotate(-180deg)}.wd-collapse-item__wrapper{position:relative;transition:height .3s ease-in-out;overflow:hidden;will-change:height}.wd-collapse-item__body{color:rgba(0,0,0,.65);font-size:14px;padding:14px 25px;line-height:1.43}.wd-collapse-item.is-disabled .wd-collapse-item__arrow,.wd-collapse-item.is-disabled .wd-collapse-item__title{color:rgba(0,0,0,.15)}