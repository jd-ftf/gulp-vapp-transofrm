.wd-loading{font-size:0;line-height:0;vertical-align:middle;display:inline-block}.wd-loading__spinner{display:block;width:100%;height:100%;animation:a .8s linear infinite}.wd-loading--circle{animation-duration:2s}.wd-loading--circle-outline{width:100%;height:100%;animation:a .8s linear infinite;animation-duration:2s}.wd-loading__svg{width:100%;height:100%;background-size:cover;background-repeat:no-repeat}.wd-loading--spinner{animation-timing-function:steps(12)}.wd-loading__spinner-dot{position:absolute;top:0;left:0;width:100%;height:100%}.wd-loading__spinner-dot:before{display:block;content:"";width:2px;height:25%;border-radius:1px;margin:0 auto;background:currentColor}.wd-loading__spinner-dot:first-of-type{opacity:1;transform:rotate(30deg)}.wd-loading__spinner-dot:nth-of-type(2){opacity:.9375;transform:rotate(60deg)}.wd-loading__spinner-dot:nth-of-type(3){opacity:.875;transform:rotate(90deg)}.wd-loading__spinner-dot:nth-of-type(4){opacity:.8125;transform:rotate(120deg)}.wd-loading__spinner-dot:nth-of-type(5){opacity:.75;transform:rotate(150deg)}.wd-loading__spinner-dot:nth-of-type(6){opacity:.6875;transform:rotate(180deg)}.wd-loading__spinner-dot:nth-of-type(7){opacity:.625;transform:rotate(210deg)}.wd-loading__spinner-dot:nth-of-type(8){opacity:.5625;transform:rotate(240deg)}.wd-loading__spinner-dot:nth-of-type(9){opacity:.5;transform:rotate(270deg)}.wd-loading__spinner-dot:nth-of-type(10){opacity:.4375;transform:rotate(300deg)}.wd-loading__spinner-dot:nth-of-type(11){opacity:.375;transform:rotate(330deg)}.wd-loading__spinner-dot:nth-of-type(12){opacity:.3125;transform:rotate(1turn)}@keyframes a{0%{transform:rotate(0deg)}to{transform:rotate(1turn)}}