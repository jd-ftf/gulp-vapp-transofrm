.wd-cell{position:relative;padding-left:15px;background-color:#fff;text-decoration:none;color:rgba(0,0,0,.85);line-height:1.43;-webkit-tap-highlight-color:transparent}.wd-cell.is-border .wd-cell__wrapper:after{position:absolute;display:block;content:"";width:100%;height:1px;left:0;top:0;transform:scaleY(.5);background:#e8e8e8;z-index:1}.wd-cell__wrapper{position:relative;display:flex;padding:13px 15px 13px 0;justify-content:space-between;align-items:flex-start}.wd-cell__wrapper.is-vertical{display:block}.wd-cell__wrapper.is-vertical .wd-cell__right{margin-top:16px}.wd-cell__wrapper.is-vertical .wd-cell__value{text-align:left}.wd-cell__wrapper.is-label{padding:16px 15px 16px 0}.wd-cell__left{position:relative;flex:1;display:flex;margin-right:15px;font-size:14px;box-sizing:border-box}.wd-cell__left.is-required{padding-left:12px}.wd-cell__left.is-required:after{position:absolute;content:"*";top:0;left:0;font-size:18px;color:#fa4350}.wd-cell__right{position:relative;display:flex;align-items:center;flex:1}.wd-cell__title{flex:1;width:100%;font-size:14px;margin-right:15px}.wd-cell__label{margin-top:2px;font-size:12px;color:rgba(0,0,0,.45);overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.wd-cell__icon{display:block;position:relative;width:16px;height:16px;line-height:1.25;margin-right:15px;font-size:16px}.wd-cell__value{position:relative;flex:1;font-size:12px;color:rgba(0,0,0,.45);text-align:right;line-height:1.67}.wd-cell__arrow-right{display:inline-block;margin-left:8px;width:16px;height:16px;line-height:16px;font-size:16px;color:rgba(0,0,0,.25);vertical-align:middle}.wd-cell.is-link{-webkit-tap-highlight-color:rgba(0,0,0,.06)}.wd-cell.is-hover{background-color:rgba(0,0,0,.06)}.wd-cell.is-large .wd-cell__title{font-size:16px}.wd-cell.is-large .wd-cell__label{font-size:14px}.wd-cell.is-large .wd-cell__value{font-size:14px;line-height:1.58}.wd-cell.is-large .wd-cell__arrow-right,.wd-cell.is-large .wd-cell__icon{font-size:18px;width:18px;height:18px}.wd-cell.is-ce