.wd-message-box{border-radius:16px;overflow:hidden}.wd-message-box__container{width:300px;box-sizing:border-box}.wd-message-box__body{background-color:#fff;padding:25px 24px 0}.wd-message-box__body.is-no-title{padding:25px 24px 0}.wd-message-box__title{text-align:center;font-size:16px;color:rgba(0,0,0,.85);line-height:20px;font-weight:500;padding-top:5px;padding-bottom:10px}.wd-message-box__content{max-height:264px;color:#666;font-size:14px;text-align:center;overflow:auto;word-break:break-all;line-height:20px}.wd-message-box__content::-webkit-scrollbar{width:4px}.wd-message-box__content::-webkit-scrollbar-thumb{width:4px;background:rgba(0,0,0,.1);border-radius:2px}.wd-message-box__input-error{min-height:18px;margin-top:2px;color:#fa4350;text-align:left}.wd-message-box__input-error.is-hidden{visibility:hidden}.wd-message-box__actions{padding:24px}.wd-message-box__flex{display:flex}.wd-message-box__block{display:block}.wd-message-box__cancel{margin-right:16px}.zoomIn-enter-active,.zoomIn-leave-active{opacity:1;transform:translate3d(-50%,-50%,0) scale(1);transition:all .2s}.zoomIn-enter{transform:translate3d(-50%,-50%,0) scale(.7)}.zoomIn-enter,.zoomIn-leave-to{opacity:0;transition:all .2s ease-out}.zoomIn-leave-to{transform:translate3d(-50%,-50%,0) scale(.9)}