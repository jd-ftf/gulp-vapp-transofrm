.wd-slider{display:flex;flex-flow:row nowrap;align-items:center;height:36px}.wd-slider__label,.wd-slider__label-max,.wd-slider__label-min{font-size:14px;color:#333}.wd-slider__label{text-align:center;width:24px;line-height:24px;line-height:1.2;background-color:hsla(0,0%,100%,.5);border-radius:100%;position:absolute;bottom:32px}.wd-slider__bar-wrapper{flex:1;position:relative;border-radius:1.5px;background-color:#e5e5e5;margin:10.5px 0}.wd-slider__bar{position:relative;border-radius:inherit;height:3px;background:linear-gradient(315deg,#517cf0,#769ef5)}.wd-slider__button-wrapper{width:24px;position:absolute;right:0;top:0;transform:translate3d(-50%,-50%,0)}.wd-slider__has-label{padding-top:24.8px}.wd-slider__button{height:24px;width:24px;background:linear-gradient(139deg,#fff,#f7f7f7);border-radius:100%;border:1px solid #e5e5e5;box-sizing:border-box;box-shadow:0 2px 4px 0 hsla(0,0%,61%,.5)}.wd-slider__label-min{margin-right:23px}.wd-slider__label-max{margin-left:23px}.wd-slider--disabled .wd-slider__bar{opacity:.25}.wd-slider--disabled .wd-slider__label-max,.wd-slider--disabled .wd-slider__label-min{color:rgba(0,0,0,.25)}