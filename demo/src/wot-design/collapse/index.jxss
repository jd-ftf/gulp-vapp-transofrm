.wd-collapse{background:#fff}.wd-collapse.is-viewmore{padding:15px}.wd-collapse__content{font-size:14px;color:rgba(0,0,0,.65)}.wd-collapse__content.is-retract{display:-webkit-box;overflow:hidden;font-size:14px}.wd-collapse__more{font-size:14px;margin-top:8px;color:#4d80f0;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}.wd-collapse__more-txt{display:inline-block;vertical-align:middle;margin-right:4px}.wd-collapse__arrow{display:inline-block;vertical-align:middle;transition:transform .1s;font-size:18px;height:18px;line-height:18px}.wd-collapse__arrow.is-retract{transform:rotate(-180deg)}